
{% extends "huma.demo9.base.html.twig" %}

{% block title %}Realisations : Modifier Realisation :  {% endblock %}
{% block stylesheets %}
{{ parent() }}  
        <!-- Flatpickr -->
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr.css')}}" rel="stylesheet">
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr.rtl.css')}}" rel="stylesheet">
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr-airbnb.css')}}" rel="stylesheet">
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr-airbnb.rtl.css')}}" rel="stylesheet">
    <!-- DateRangePicker -->
     <link type="text/css" href="{{ asset('build/assets/vendor/daterangepicker.css')}}" rel="stylesheet">

{% endblock %}

{% block body %}


    <div class="preloader">
        <div class="sk-double-bounce">
            <div class="sk-child sk-double-bounce1"></div>
            <div class="sk-child sk-double-bounce2"></div>
        </div>
    </div>

    <div class="mdk-drawer-layout js-mdk-drawer-layout" data-push data-responsive-width="992px">
        <div class="mdk-drawer-layout__content page-content">

            <!-- Header -->




             {% block MainHeader %}
			 
				{{ parent() }}  
             {% endblock %}



            <!-- // END Header -->
            
            <!-- //  Page Header -->
            {% block PageHeader %}
			<div class="border-bottom-2 py-32pt position-relative z-1">
                <div class="container-fluid page__container d-flex flex-column flex-md-row align-items-center text-center text-sm-left">
                    <div class="flex d-flex flex-column flex-sm-row align-items-center mb-24pt mb-md-0">

                        <div class="mb-24pt mb-sm-0 mr-sm-24pt">
                            <h2 class="mb-0">Realisations</h2>


                            <ol class="breadcrumb p-0 m-0">
                                <li class="breadcrumb-item"><a href="index.html">Home</a></li>

                                <li class="breadcrumb-item active">

                                    Realisations

                                </li>

                            </ol>

                        </div>

                        
                    </div>


                    <div class="row" role="tablist">
                        <div class="col-auto d-flex flex-column">

                        <form   class="search-form sidebar-m-b sidebar-p-l mx-16pt pr-0 search-form--light">
                            <input type="text" class="form-control pl-0" placeholder="Rechercher un Realisations" name="q" value="{{ app.request.query.get('q') }}">
                            <button class="btn" type="submit"><i class="material-icons">search</i></button>
                         </form>
 
                        </div>
                        {% if is_granted('ROLE_ADMIN') or is_granted('ROLE_TUTEUR') %}
                        <div class="col-auto border-left">
                            <a href="" class="btn btn-accent">Nouvelle Realisation</a>
                        </div>
                        {% endif  %}
                    </div>

                </div>
            </div>
				
            {% endblock %}
            <!-- // END Page Header -->
             
             <!-- //   Page Content -->
            
				<div class="container-fluid page__container page__container page-section">
                  
                 {% block Content %}
                 
				  <div class="row">
                  {{ parent() }}
				  		<div class="col-md-8">

                               <div class="page-separator">
										<div class="page-separator__text"> Informations Du Realisations 
                                           
                                        </div>
									</div>
                                  {{ include('realisation/_delete_form.html.twig') }}
                                  {{ include('realisation/_form.html.twig', {'button_label': 'Modifier'}) }}



                                    

                                

                          
                                        

                            

                             
                        </div>

						 <div class="col-lg-4">
							 




									<!-- card-->
								<div class="card mb-lg-0">
		                                <div class="card-header d-flex align-items-center">
		                                    <strong class="flex">Tuteur</strong>
 		                                </div>
		                                <div class="progress rounded-0" style="height: 4px;">
		                                    <div class="progress-bar bg-accent" role="progressbar" style="width: 40%;" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
		                                </div>
		                                <div class="list-group list-group-flush">
										<div class="card card-body p-lg-24pt">
    										<div class="d-flex align-items-center">
                                        		<div class="mr-3">
                                            			<div class="avatar avatar-xl">
														    {% if vich_uploader_asset(app.user, 'imageFile') is null %}
															<img src="https://avatars.dicebear.com/v2/initials/{{ user.firstname }}%20{{ user.lastname }}.svg"  alt="Avatar" class="rounded-circle mr-8pt" >
															{% else %}
																<img src="{{ vich_uploader_asset(user, 'imageFile') }}" class="rounded-circle mr-8pt"><br>
															{% endif %}
                                             			</div>
                                        		 </div>
                                            <div class="flex">
                                           		 <h4 class="mb-0">{{user.firstname}} {{user.lastname}}</h4>
                                            	 <p class="text-50 mb-0">{{user.about}}</p>
                                        	</div>
                                    	</div>
                                 </div>
								 <!-- end card-->




		                                    
											


		                                </div>
		                            </div>
									<br/>
									<!-- // Realisateurs -->
									<div class="page-separator">
										<div class="page-separator__text">Realisateurs   </div>
									</div>

									<div class="mb-4">
									   <div class="card card-body">
												<div class="d-flex align-items-center mb-2">
														<a href="" class="avatar avatar-xs mr-8pt">
															<img src="http://luma.humatheme.com/build/assets/images/256_michael-dam-258165-unsplash.jpg" alt="Realisationse" class="avatar-img rounded-circle">
														</a>
														<a href="" class="flex mr-2 text-body"><strong>Luci Bryant</strong></a>
														
												 </div>
												 <div class="page-separator">
 												 </div>
												 <div class="d-flex align-items-center mb-2">
														<a href="" class="avatar avatar-xs mr-8pt">
															<img src="http://luma.humatheme.com/build/assets/images/256_michael-dam-258165-unsplash.jpg" alt="Realisationse" class="avatar-img rounded-circle">
														</a>
														<a href="" class="flex mr-2 text-body"><strong>Luci Bryant</strong></a>
														
												 </div>
												 <div class="page-separator">
 												 </div>
												 <div class="d-flex align-items-center mb-2">
														<a href="" class="avatar avatar-xs mr-8pt">
															<img src="http://luma.humatheme.com/build/assets/images/256_michael-dam-258165-unsplash.jpg" alt="Realisationse" class="avatar-img rounded-circle">
														</a>
														<a href="" class="flex mr-2 text-body"><strong>Luci Bryant</strong></a>
														
												 </div>
										</div>
												 
									</div>
									
									 <!-- // End Realisateurs -->
									
						</div>

							  

							 

						
				  </div>

               

                
            </div>
            {% endblock %}
            <!-- // END content -->


            <!-- // Footer -->
             {% block footer %}
				{{ parent() }}  
            {% endblock %}
             <!-- // END footer -->
            


        </div>
        <!-- // END drawer-layout__content -->

        {% block drawerSideBar %}
			{{ parent() }}  
        {% endblock %}
        {% block Settings %}
		{{ parent() }}  
        {% endblock %}


    {% block javascripts %}
	{{ parent() }}  
       <!-- DateRangePicker -->
    <script src="{{ asset('build/assets/vendor/moment.min.js') }}"></script>
    <script src="{{ asset('build/assets/vendor/daterangepicker.js') }}"></script>
    <script src="{{ asset('build/assets/js/daterangepicker.js') }}"></script>

    <script src="{{ asset('build/js/ckeditor/ckeditor/adapters/jquery.js') }}" type="text/javascript"></script>
    <script src="{{ asset('build/js/ckeditor/ckeditor/ckeditor.js') }}" type="text/javascript"></script>
    <script type="text/javascript">
        $(function() {
            $('textarea.ckeditor').ckeditor();
        });
    </script>
    {% endblock %}
 

 {% endblock %}

 








