{% extends "huma.demo9.base.html.twig" %}

{% block title %}Utilisateurs : Ajouter Utilisateur {% endblock %}
{% block stylesheets %}
{{ parent() }}  
        <!-- Flatpickr -->
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr.css')}}" rel="stylesheet">
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr.rtl.css')}}" rel="stylesheet">
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr-airbnb.css')}}" rel="stylesheet">
    <link type="text/css" href="{{ asset('build/assets/css/flatpickr-airbnb.rtl.css')}}" rel="stylesheet">
    <!-- DateRangePicker -->
     <link type="text/css" href="{{ asset('build/assets/vendor/daterangepicker.css')}}" rel="stylesheet">

{% endblock %}

{% block body %}


    <div class="preloader">
        <div class="sk-double-bounce">
            <div class="sk-child sk-double-bounce1"></div>
            <div class="sk-child sk-double-bounce2"></div>
        </div>
    </div>

    <div class="mdk-drawer-layout js-mdk-drawer-layout" data-push data-responsive-width="992px">
        <div class="mdk-drawer-layout__content page-content">

            <!-- Header -->




             {% block MainHeader %}
			 
				{{ parent() }}  
             {% endblock %}



            <!-- // END Header -->
            
            <!-- //  Page Header -->
            {% block PageHeader %}
			<div class="border-bottom-2 py-32pt position-relative z-1">
                <div class="container-fluid page__container d-flex flex-column flex-md-row align-items-center text-center text-sm-left">
                    <div class="flex d-flex flex-column flex-sm-row align-items-center mb-24pt mb-md-0">

                        <div class="mb-24pt mb-sm-0 mr-sm-24pt">
                            <h2 class="mb-0">Ajouter Utilisateur</h2>


                            <ol class="breadcrumb p-0 m-0">
                                <li class="breadcrumb-item"><a href="index.html">Acceuil</a></li>

                                <li class="breadcrumb-item active">

                                    Utilisateurs

                                </li>

                            </ol>

                        </div>

                        
                    </div>


                   
                   

                </div>
            </div>
				
            {% endblock %}
            <!-- // END Page Header -->
             
             <!-- //   Page Content -->
            
				<div class="container-fluid page__container page__container page-section">
                  
                 {% block Content %}
                  {{ parent() }}  
				   {{ include('admin/user/_form.html.twig') }}

               

                
            </div>
            {% endblock %}
            <!-- // END content -->


            <!-- // Footer -->
             {% block footer %}
				{{ parent() }}  
            {% endblock %}
             <!-- // END footer -->
            


        </div>
        <!-- // END drawer-layout__content -->

        {% block drawerSideBar %}
			{{ parent() }}  
        {% endblock %}
        {% block Settings %}
		{{ parent() }}  
        {% endblock %}


    {% block javascripts %}
	{{ parent() }}  
          <!-- DateRangePicker -->
    <script src="{{ asset('build/assets/vendor/moment.min.js') }}"></script>
    <script src="{{ asset('build/assets/vendor/daterangepicker.js') }}"></script>
    <script src="{{ asset('build/assets/js/daterangepicker.js') }}"></script>
    {% endblock %}
 

 {% endblock %}

 
